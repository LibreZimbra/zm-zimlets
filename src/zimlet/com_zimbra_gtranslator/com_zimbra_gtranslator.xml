<zimlet name="com_zimbra_gtranslator" version="2.0" label="Google Translator" description="Translates drag-and-drop email messages using Google Translator.">
	<include>gtranslator.js</include>
	<includeCSS>gtranslator.css</includeCSS>
	<resource>google-icon.gif</resource>
	<handlerObject>com_zimbra_gtranslator_HandlerObject</handlerObject>
	<zimletPanelItem label="${msg.GTranslatorZimlet_panel_label}" icon="Google-panelIcon">
		<toolTipText>${msg.GTranslatorZimlet_panel_tooltip}</toolTipText>
		<dragSource type="ZmConv" />
		<dragSource type="ZmMailMsg" />
	</zimletPanelItem>
</zimlet>
