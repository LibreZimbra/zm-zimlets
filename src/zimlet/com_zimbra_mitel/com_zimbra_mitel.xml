<zimlet name="com_zimbra_mitel" version="0.1"  label="${msg.label}" description="${msg.description}">
	<include>ZmMitel.js</include>
	<include>base64.js</include>
	<include>ZmMitelSoapCalls.js</include>
	<include>ZmMitelClick2CallDlg.js</include>
	<include>ZmMitelFromPhoneDlg.js</include>
	<include>templates/ZmMitel.template.js</include>
	<includeCSS>ZmMitel.css</includeCSS>
	<handlerObject>ZmMitelZimlet</handlerObject>
	<contentObject type="phone">
		<matchOn>
			<regex attrs="g">(((\+\d{2}[-\. ]\d{5}[-\. ]?\d{5})|(1\d{10})|(\d{10})|(\b(\d{1}[-. ])?(\(?\d{3}\)?[-. ])?\d{3}[-.]\d{4}\b)|(\(\d{3}\)[-\. ]?\d{7}))(( x| ext)[-#: ]?\d{1,5}){0,1})</regex>
		</matchOn>
	</contentObject>
</zimlet>
